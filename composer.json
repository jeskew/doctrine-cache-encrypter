{
    "name": "jeskew/doctrine-cache-encrypter",
    "description": "An encrypting decorator for instances of Doctrine\\Common\\Cache\\Cache",
    "minimum-stability": "stable",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Jonathan Eskew",
            "email": "jonathan@jeskew.net"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-openssl": "*",
        "doctrine/cache": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "Jeskew\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jeskew\\Cache\\": "tests/"
        }
    }
}
